{
	"name": "incrementaldata",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorageLinkedService",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat('data_',formatDateTime(convertTimeZone(utcNow(),'UTC','India Standard Time'),'yyyMMdd_HHmmss'),'.csv')",
					"type": "Expression"
				},
				"folderPath": "Supriya/Transactional/incrementoutput",
				"fileSystem": "rawdata"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}